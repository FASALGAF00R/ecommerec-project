<script>
	const addcart = function (id) {
		console.log(id,"kk");
		
		$.ajax({
			url: "/addtocart",
			method: 'get',
			data: {
				id: id
			},
			success: function (response) {
				if(response.success){

					Swal.fire(
						'Saved cart!',
						'You clicked the button!',
						'success'
					)
				}else{
					 swal.fire("please Login to your account")
				}
				
			}
			
			
		})
	}
	




</script>

<script>

	function deletewhishlist(id) {
		$.ajax({
			url: "/removewish",
			method: "post",
			encoded: true,
			data: {
				id: id
			}
		}).done((data) => {
			console.log(data);
			if (data.success === true) {
				window.location.reload()
			}
		})
	}

</script>

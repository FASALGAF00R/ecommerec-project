											<!-- <div class="media-body">
												<p>
												
												</p>

											</div> -->
											<td>
												<%=product.productname  %>
											</td>
										</div>
									</td>
									<td>
										<a href="" class="genric-btn danger circle" id="delete"
											onclick="deletewhishlist('<%= product.productId._id%>')">Remove</a>
											<br>
											<br>
										 <a href="#" class="genric-btn danger circle" id="add"
										  onclick="addcart('<%= product.productId._id%>')">Add to cart</a>
									</td>
								   
									

									<td>
										<h5>$<%=product.price%>
										</h5>

									</td>

								</tr>
								<% }) %>
